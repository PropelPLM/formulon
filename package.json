{
  "name": "formulon",
  "version": "0.1.0",
  "description": "Salesforce Formula Parser",
  "main": "lib/formulon.js",
  "scripts": {
    "build": "pegjs ./grammars/salesforceFormula.pegjs ./lib/salesforceParser.js && babel src --out-dir lib",
    "compile:parser": "pegjs ./grammars/salesforceFormula.pegjs ./lib/salesforceParser.js",
    "compile:parser:test": "pegjs ./grammars/salesforceFormula.pegjs ./src/salesforceParser.js",
    "test": "npm run compile:parser:test && mocha --compilers js:babel-register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leifg/formulon.git"
  },
  "keywords": [
    "parser",
    "salesforce",
    "formula"
  ],
  "author": "Leif Gensert",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "pegjs": "^0.9.0",
    "pegjs-loader": "^0.4.0",
    "webpack": "^1.12.13"
  }
}
